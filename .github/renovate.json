{
    "extends": ["config:base"],
    "labels": ["dependencies"],
    "masterIssue": true,
    "packageRules": [
        {
            "updateTypes": ["minor", "patch", "pin", "digest"],
            "automerge": true
        }
    ],
    "schedule": ["after 10pm and before 5am on every weekday", "every weekend"],
    "semanticCommits": true,
    "stabilityDays": 3,
    "rebaseStalePrs": true,
    "timezone": "Europe/Vienna",
    "vulnerabilityAlerts": {
        "labels": ["security"],
        "assignees": ["@biegl"]
    },
    "encrypted": {
        "composerUsername": "",
        "composerPassword": ""
    },
    "hostRules": [
        {
          "hostName": "nova.laravel.com",
          "username": "${COMPOSER_USERNAME}",
          "password": "${COMPOSER_PASSWORD}"
        }
    ]
}
